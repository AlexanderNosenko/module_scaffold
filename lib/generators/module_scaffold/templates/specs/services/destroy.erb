require 'rails_helper'

describe ::<%= @helper.full_class_name('destroy') %> do
  subject { described_class.new(<%= @helper.resource_name %>).call }

  let_it_be(:<%= @helper.resource_name %>) { create(:<%= @helper.resource_name %>) }

  describe '#call' do
    it 'deletes <%= @helper.resource_name %>' do
      expect { subject }.to change { ::<%= @helper.module_full_name %>.count }.by(-1)
    end
  end
end
